{
  "name": "Bibleify",
  "version": "1.0.0",
  "description": "Simple Bible Desktop App with Dramatized Audio",
  "main": "main.js",
  "scripts": {
    "eslint": "eslint 'src/**/*.js'",
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds --icon=./appicon.icns"
  },
  "repository": "https://github.com/sonnylazuardi/bibleify-desktop",
  "keywords": [
    "Electron",
    "Bible",
    "Desktop",
    "Audio",
    "Realm",
    "Clappr",
    "React",
    "Rematch"
  ],
  "author": "sonnylazuardi",
  "license": "MIT",

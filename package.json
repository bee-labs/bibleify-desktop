    "@rematch/core": "^0.5.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "bootstrap": "^4.0.0",
    "clappr": "^0.2.87",
    "css-loader": "^0.28.10",
    "electron-prompt": "^0.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
